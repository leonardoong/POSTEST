<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="tombolHijau" parent="Widget.AppCompat.Button.Colored">
        <item name="backgroundTint">@color/warnaTombol</item>
        <item name="android:textColor">@color/textTombol</item>
    </style>
    <style name="tombolHijauMuda" parent="Widget.AppCompat.Button.Colored">
        <item name="backgroundTint">@color/warnaTombolMuda</item>
        <item name="android:textColor">@color/textTombol</item>
    </style>

    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>
</resources>
